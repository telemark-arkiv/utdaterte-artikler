    "start": "node standalone.js",
    "setup": "npm install && node setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/telemark/utdaterte-artikler.git"
  },
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "coveralls": "2.11.15",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "8.0.1"
  },
  "dependencies": {
    "concat-stream": "1.5.2",
    "handlebars": "4.0.6",
    "hapi": "15.2.0",
    "hoek": "4.1.0",
    "http-https": "1.0.0",
    "inert": "4.0.1",
    "sitemap-to-array": "2.0.0",
    "vision": "4.1.0",
    "wreck": "10.0.0",
    "yar": "8.1.0"
