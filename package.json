    "standard": "6.0.8",
    "tap": "5.7.0"
  },
  "dependencies": {
    "concat-stream": "1.5.1",
    "handlebars": "4.0.5",
    "hapi": "13.3.0",
    "hoek": "3.0.4",
    "http-https": "1.0.0",
    "inert": "3.2.0",
    "sitemap-to-array": "4.0.0",
    "vision": "4.0.1",
    "wreck": "7.1.0",
    "yar": "7.0.0"
